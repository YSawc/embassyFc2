interface RegisterIf {
    var a : logic<8> ;
    var x : logic<8> ;
    var y : logic<8> ;
    var s : logic<8> ;
    var p : logic<8> ;
    var pc: logic<16>;

    modport Master {
        a : output,
        x : output,
        y : output,
        s : output,
        p : output,
        pc: output,
    }

    modport Slave {
        a : input,
        x : input,
        y : input,
        s : input,
        p : input,
        pc: input,
    }
}
